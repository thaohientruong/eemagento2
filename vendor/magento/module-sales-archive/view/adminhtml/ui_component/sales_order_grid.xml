<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <argument name="data" xsi:type="array">
        <item name="buttons" xsi:type="array">
            <item name="go_to_archive" xsi:type="array">
                <item name="name" xsi:type="string">go_to_archive</item>
                <item name="label" xsi:type="string" translate="true">Go to Archive</item>
                <item name="url" xsi:type="string">sales/archive/orders</item>
            </item>
        </item>
    </argument>
    <container name="listing_top">
        <massaction name="listing_massaction">
            <action name="add_order_to_archive">
                <argument name="data" xsi:type="array">
                    <item name="config" xsi:type="array">
                        <item name="type" xsi:type="string">add_order_to_archive</item>
                        <item name="label" xsi:type="string" translate="true">Move to Archive</item>
                        <item name="url" xsi:type="url" path="sales/archive/massAdd"/>
                    </item>
                </argument>
            </action>
        </massaction>
    </container>
</listing>
